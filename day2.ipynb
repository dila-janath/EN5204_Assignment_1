    "import cv2 as cv\n",
    "\n",
    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "im = np.zeros((6,8),dtype=np.uint8)\n",
    "im[2,3] = 200\n",
    "\n",
    "fig,ax = plt.subplots()\n",
    "ax.imshow(im, cmap = 'gray', vmin=0, vmax=255)\n",
    "ax.xaxis.tick_top()\n",
    "plt.show\n"
   ]
  }
